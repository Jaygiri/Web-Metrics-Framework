<?xml version="1.0"?>
<RDF xmlns="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
    xmlns:em="http://www.mozilla.org/2004/em-rdf#">

  <Description about="urn:mozilla:install-manifest">
   
    <em:id>wmf@javascriptrules.com</em:id>
    <em:name>Web Metrics Framework</em:name>
    <em:version>0.1.1</em:version>
    <em:description>YSlow extension to measure how good or bad a page is according to Google web metrics</em:description>
    <em:creator>Marcel Duran</em:creator>
    
    <em:homepageURL>http://labs.javascriptrules.com/wmf</em:homepageURL>
    <em:iconURL>chrome://wmf/content/icon.png</em:iconURL>
    
    <!-- Firefox -->
    <em:targetApplication>
      <Description>
        <em:id>{ec8030f7-c20a-464f-9b0e-13a3a9e97384}</em:id>
        <em:minVersion>3.0</em:minVersion>
        <em:maxVersion>3.6.*</em:maxVersion>
      </Description>
    </em:targetApplication>

    <!-- YSlow -->
    <em:requires>
       <Description>
         <em:id>yslow@yahoo-inc.com</em:id>
         <em:minVersion>2.0.0</em:minVersion>
         <em:maxVersion>2.0.*</em:maxVersion>
       </Description>
    </em:requires>
    
  </Description>

</RDF>
